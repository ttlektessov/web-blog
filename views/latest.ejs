<%- include("partials/header.ejs") %>

<div class="latest-container">
  <h2>Latest Post</h2>
  <% if (latestPost) { %>
    <h3><%= latestPost.title %></h3>
    <p><%= latestPost.body %></p>
    <small><%= latestPost.date %></small>
  <% } else { %>
    <p>No posts available.</p>
  <% } %>
</div>

